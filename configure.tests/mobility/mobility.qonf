<?xml version="1.0" ?>
<qonf optional="false" >
	<test name="Qt Mobility">
		<dir path="." />
	</test>
	<provide>
		<defines>
			<define value="HAVE_MOBILITY" />
		</defines>
		<libs>
			<lib path="-lQtMultimediaKit" />
		</libs>
	</provide>
	<solution>
		<linux package="qtmobility" url="http://doc.qt.digia.com/qtmobility" comment="" >
			<distribution name="gentoo" package="qtmobility" url="http://doc.qt.digia.com/qtmobility" comment="Try: emerge qt-mobility" />
			<distribution name="ubuntu" package="qtmobility-dev" url="http://doc.qt.digia.com/qtmobility" comment="Try: apt-get install qtmobility-dev" />
		</linux>
		<windows package="qtmobility" url="http://doc.qt.digia.com/qtmobility" comment="" />
		<macosx package="qtmobility" url="http://doc.qt.digia.com/qtmobility" comment="" />
	</solution>
</qonf>
